{
  "name": "WonderApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts*'",
    "test": "jest",
    "dev": "react-native run-ios --simulator \"iPhone 5s\"",
    "dev:md": "react-native run-ios --simulator \"iPhone 7\"",
    "dev:lg": "react-native run-ios --simulator \"iPhone X\"",
    "run:android": "react-native run-android",
    "build": "node ./scripts/buildWithPort.js",
    "nukeYarn": "rm -f yarn.lock && yarn",
    "nukePods": "cd ios && rm -rf Pods/ && rm -f Podfile.lock && pod update && cd ../",
    "postMerge": "yarn run nukeYarn && yarn run nukePods && yarn run cleantp",
    "start": "node node_modules/react-native/local-cli/cli.js start --reset-cache --port",
    "killwatch": "watchman watch-del-all",
    "killnode": "killall -9 node",
    "reverse": "adb reverse tcp:8081 tcp:8081",
    "xc": "node ./scripts/openXc.js",
    "cleanAndroid": "./android/gradlew clean -p ./android/ && rm -rf android/build/ && rm -rf android/app/build/",
    "cleanIos": "rm -rf ios/build/ && rm -rf ~/Library/Developer/Xcode/DerivedData/*",
    "cleantp": "rm -rf node_modules/react-native/third-party/",
    "generate-ios-bundle": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios-release.bundle --sourcemap-output ios-release.bundle.map",
    "generate-android-bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android-release.bundle --sourcemap-output android-release.bundle.map",
    "postInstall": "rndebugger-open"
  },
  "dependencies": {
    "@ptomasroos/react-native-multi-slider": "^1.0.0",
    "appcenter": "~1.8.1",
    "appcenter-analytics": "~1.8.1",
    "appcenter-crashes": "~1.8.1",
    "appcenter-push": "~1.8.1",
    "axios": "^0.18.0",
    "bugsnag-react-native": "^2.12.4",
    "color": "^3.0.0",
    "lodash": "^4.17.10",
    "moment-timezone": "^0.5.21",
    "native-base": "^2.7.2",
    "react": "16.6.3",
    "react-native": "0.57.8",
    "react-native-actioncable": "0.0.3",
    "react-native-animatable": "^1.3.0",
    "react-native-calendar-events": "^1.6.3",
    "react-native-calendars": "^1.19.4",
    "react-native-device-info": "^0.21.5",
    "react-native-fast-image": "5.1.1",
    "react-native-fbsdk": "^0.8.0",
    "react-native-gifted-chat": "^0.4.3",
    "react-native-google-analytics": "^1.3.2",
    "react-native-image-picker": "^0.26.10",
    "react-native-image-rotate": "^2.1.0",
    "react-native-linear-gradient": "2.4.2",
    "react-native-maps": "^0.21.0",
    "react-native-multi-slider": "https://github.com/wkoutre/react-native-multi-slider-1.git",
    "react-native-popup-menu": "^0.14.0",
    "react-native-push-notification": "^3.1.1",
    "react-native-range-slider": "^0.1.0",
    "react-native-searchbar": "^1.15.0",
    "react-native-slider": "^0.11.0",
    "react-native-svg": "^6.5.2",
    "react-native-svg-uri": "^1.2.3",
    "react-native-vector-icons": "^4.6.0",
    "react-native-version-number": "^0.3.4",
    "react-native-video": "^3.2.0",
    "react-native-video-player": "^0.9.1",
    "react-navigation": "^2.6.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-actions": "^2.4.0",
    "redux-enhancer-react-native-appstate": "^0.3.1",
    "redux-persist": "^5.10.0",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "validator": "^10.4.0"
  },
  "devDependencies": {
    "@types/color": "^3.0.0",
    "@types/jest": "^23.1.2",
    "@types/lodash": "^4.14.111",
    "@types/moment-timezone": "^0.5.6",
    "@types/react": "^16.4.2",
    "@types/react-native": "^0.55.25",
    "@types/react-native-calendars": "^1.20.1",
    "@types/react-native-fbsdk": "^0.6.2",
    "@types/react-native-push-notification": "^3.0.2",
    "@types/react-native-vector-icons": "^4.6.1",
    "@types/react-navigation": "^2.0.8",
    "@types/react-redux": "^6.0.3",
    "@types/react-test-renderer": "^16.0.1",
    "@types/redux-actions": "^2.3.0",
    "@types/validator": "^9.4.1",
    "babel-jest": "23.2.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-react-native": "^4.0.1",
    "jest": "23.2.0",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.3.1",
    "reactotron-react-native": "^2.0.0",
    "reactotron-redux": "^2.0.0",
    "reactotron-redux-saga": "^2.0.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.11.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.2"
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts/"
    ]
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache",
    "setupFiles": [
      "<rootDir>/node_modules/appcenter/test/AppCenterMock.js",
      "<rootDir>/node_modules/appcenter-analytics/test/AppCenterAnalyticsMock.js",
      "<rootDir>/node_modules/appcenter-crashes/test/AppCenterCrashesMock.js",
      "<rootDir>/node_modules/appcenter-push/test/AppCenterPushMock.js"
    ]
  }
}
